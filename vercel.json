{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/",
            "methods":["GET","POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers":{
                "Access-Control-Allow-Origin": "*"
                
            }
        }
    ],
    "headers": [
        {
          "source": "/(.*)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "public, max-age=3600"
            },
            {
              "key": "Content-Security-Policy",
              "value": "" 
            },
            {
              "key": "X-XSS-Protection",
              "value": "0" 
            }
          ]
        }
      ]
}